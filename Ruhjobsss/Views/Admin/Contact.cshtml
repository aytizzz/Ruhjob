@model List<Ruhjobsss.DataAccess.Entity.Contact>
@{
    ViewData["Title"] = "Contact";
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
}


<div id="page-wrapper">
    <div id="page-inner">
        <div class="row">
            <div class="col-lg-12 col-md-6">
                <h5>Contact table</h5>
                <a href="/Admin/ContactAdd/" class="btn btn-primary">New</a>
                <table class="table table-striped table-bordered table-hover">
                    <thead>
                        <tr>
                            <th>#</th>
                            <th>Email</th>
                            <th>Telephone</th>
                            <th>Delete</th>
                        </tr>
                    </thead>
                    <tbody>
                        @foreach (var item in Model)
                        {
                            <tr>
                                <td>@item.ID</td>
                                <td>@item.Email</td>
                                <td>@item.Telephone</td>
                                <td><a href="/Admin/ContactDelete/@item.ID/" class="btn btn-danger">Delete</a></td>
                            </tr>
                        }
                    </tbody>
                </table>

            </div>
        </div>
    </div>
    <!-- /. PAGE INNER  -->
</div>